{
  "name": "model_agency",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "csv-parse": "^1.1.7",
    "fibers": "^1.0.14",
    "future": "^2.3.1",
    "highcharts-heatmap": "^0.1.2",
    "highcharts-more": "^0.1.2",
    "highcharts-treemap": "^0.1.2",
    "meteor-node-stubs": "^0.2.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-highcharts": "^10.0.0",
    "react-router": "^2.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-meteor": "^0.3.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.7.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "meteor"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        {
          "functions": "ignore"
        }
      ],
      "max-len": "off",
      "no-use-before-define": [
        2,
        {
          "functions": false
        }
      ],
      "prefer-const": 1,
      "prefer-template": "off"
    },
    "settings": {
      "import/resolver": {
        "meteor": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    }
  }
}
